{{wrap_tag}}.title {
    .headline.{{wrap_tag}} @data-{{wrap_tag}}="song" "you are listenting to" 
    span.artist.link { a@href="#" {{artist}} }
    span.song {{ song }}
    p.inline { u "Inline text" }
    p.inline { u "Inline text" }
    
    .controls {
        a.play_button @href="#" @data-autoplay="{{autoplay_info}}" "Play"
        a.buy_link @href={{buy_link}} @target="_blank" "Buy"
    }   
}
